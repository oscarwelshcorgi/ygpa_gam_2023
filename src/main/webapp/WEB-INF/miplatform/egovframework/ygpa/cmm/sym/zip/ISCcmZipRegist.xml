	<Script><![CDATA[#include "lib::common.js";
var searchKeyword;
var searchCondition;

function fnCallback(svcid, errcd, errmsg)
{
	
	gfn_hideProg(this);
	if(errCd < 0){
		gfn_Error(errcd,errmsg);		
		return;
	}
	if(svcid == "insert"){
		alert(errMsg);	
		parent.btnSearch_OnClick();
		imgClose_OnClick();
		//Go(G_Server + "/ipa/sym/cmm/zip/ISCcmZipListView.do");
	}	
