					         LEFT OUTER JOIN 
					        	P2MGR.PRT_AT_CODE_F@YGPA_P2MGR P 
					         ON 
					        	(G.PRT_AT_CODE=P.PRT_AT_CODE) 
					        	
                             WHERE 1=1 
                             
                             	<isNotEmpty property="sPrtAtCode">
					             	AND E.APP_PRT_AT_CODE = #sPrtAtCode#
					         	</isNotEmpty>
					         
					         	<isNotEmpty property="sFeeTp">
						             AND E.FEE_TP = #sFeeTp#
