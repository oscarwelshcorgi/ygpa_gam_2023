                                  , E.CMPL_YR
                                  , E.CONST_NO
                                  , E.EXMP_AGENT_CODE
                                  , E.VSSL_KEY
                                  , E.YR
                                  , E.SER_NO
                                  , E.BILL_NO
                                  , E.EXMP_AMNT
                                  , E.IN_OUT
                                  , E.BILL_USE_NO
                                  , E.FAC_CODE
                                  , E.FAC_SUB_CODE
                                  , E.IO_DT
                                  , E.DC_CODE
