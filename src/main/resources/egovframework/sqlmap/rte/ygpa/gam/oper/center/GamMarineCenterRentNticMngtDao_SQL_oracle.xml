<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="ygpa.gam.oper.center">

    <typeAlias alias="egovMap" type="egovframework.rte.psl.dataaccess.util.EgovMap"/>
    <typeAlias alias="hashMap" type="java.util.HashMap"/>
    <typeAlias alias="gamMarineCenterRentNticMngtVO" type="egovframework.rte.ygpa.gam.oper.center.service.GamMarineCenterRentNticMngtVO"/>
    
    <select id="gamMarineCenterRentNticMngtDao.selectMarineCenterRentNticList_D" parameterClass="gamMarineCenterRentNticMngtVO" resultClass="egovMap">
           SELECT A.*
           FROM (
                   SELECT  ROWNUM RNUM, S.*
                     FROM (
                           SELECT 
                                 INT_SEQ             
                                ,DEL_KIND           
                                ,JINGSU_DATE            
                                ,SUNAP_DATE         
                                ,PRT_AT_CODE            
                                ,FEE_TP             
                                ,FISCAL_YR          
                                ,BILL_NO                
                                ,ACCNT_CODE         
                                ,AGENT_CODE         
                                ,BILL_AMNT          
                                ,BILL_DT                
                                ,BILL_PRT_YN            
                                ,DUE_DATE           
                                ,RCVD_DT                
                                ,RCVD_TP                
                                ,RSLT_CODE          
                                ,EXMP_AMNT          
                                ,OVER_AMNT          
                                ,DC_AMNT                
                                ,DC_CODE                
                                ,DC_RATE                
                                ,RECPT_EPDT         
                                ,ELCT_BILL_RSLT     
                                ,BULL_INFO_INQR_DTIME
                                ,AMNT_RSN           
                                ,BZ_RGST_ID         
                                ,AGENT_NAME         
                                ,LAST               
                                ,VAT_YN             
                                ,JIRO_NO                
                                ,ELEC_PAY_NO            
                                ,CUSTOMER_MNGT_NO   
                                ,CUSTOMER_ADDR      
                                ,PAY_NAME 
                            FROM ERP_REV_COLL_F@YGPA_PORTMIS
                           WHERE 1=1
                           
                            <isNotEmpty property="sPrtAtCode">
                                AND PRT_AT_CODE = #sPrtAtCode#
                            </isNotEmpty>
                           ORDER BY INT_SEQ DESC
                ) S
             ) A
        <![CDATA[ WHERE ROWNUM <= #recordCountPerPage# and RNUM > #firstIndex# ]]>
    </select>
    
    <select id="gamMarineCenterRentNticMngtDao.selectMarineCenterRentNticListTotCnt_S" parameterClass="gamMarineCenterRentNticMngtVO" resultClass="int">
            SELECT COUNT(*) totcnt
              FROM ERP_REV_COLL_F@YGPA_PORTMIS
                           WHERE 1=1
                           
                            <isNotEmpty property="sPrtAtCode">
                                AND PRT_AT_CODE = #sPrtAtCode#
                            </isNotEmpty>
    </select>
    
</sqlMap>
