<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN"
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
        <form name="fcltyUseUnuseSearchForm">
            <field property="searchDtFr" depends="required,date">
                <arg0 key="사용시작일" resource="false"/>
                <var>
                	<var-name>datePatternStrict</var-name>
                	<var-value>yyyy-MM-dd</var-value>
                </var>
            </field>
            <field property="searchDtTo" depends="required,date">
                <arg0 key="사용종료일" resource="false"/>
				
				  <var>
                	<var-name>datePatternStrict</var-name>
                	<var-value>yyyy-MM-dd</var-value>
                </var>
            </field>
        </form>
    </formset>

</form-validation>